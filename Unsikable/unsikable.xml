<?xml version="1.0" encoding="UTF-8"?>
<project name="titanic2_4_2" default="all">
  
  
  <property file="unsikable.properties"/>
  <!-- Uncomment the following property if no tests compilation is needed -->
  <!-- 
  <property name="skip.tests" value="true"/>
   -->
  
  <!-- Compiler options -->
  
  <property name="compiler.debug" value="on"/>
  <property name="compiler.generate.no.warnings" value="off"/>
  <property name="compiler.args" value=""/>
  <property name="compiler.max.memory" value="700m"/>
  <patternset id="ignored.files">
    <exclude name="**/*.hprof/**"/>
    <exclude name="**/*.pyc/**"/>
    <exclude name="**/*.pyo/**"/>
    <exclude name="**/*.rbc/**"/>
    <exclude name="**/*.yarb/**"/>
    <exclude name="**/*~/**"/>
    <exclude name="**/.DS_Store/**"/>
    <exclude name="**/.git/**"/>
    <exclude name="**/.hg/**"/>
    <exclude name="**/.svn/**"/>
    <exclude name="**/CVS/**"/>
    <exclude name="**/__pycache__/**"/>
    <exclude name="**/_svn/**"/>
    <exclude name="**/vssver.scc/**"/>
    <exclude name="**/vssver2.scc/**"/>
  </patternset>
  <patternset id="library.patterns">
    <include name="*.egg"/>
    <include name="*.jar"/>
    <include name="*.ear"/>
    <include name="*.klib"/>
    <include name="*.swc"/>
    <include name="*.war"/>
    <include name="*.zip"/>
    <include name="*.ane"/>
  </patternset>
  <patternset id="compiler.resources">
    <exclude name="**/?*.java"/>
    <exclude name="**/?*.form"/>
    <exclude name="**/?*.class"/>
    <exclude name="**/?*.groovy"/>
    <exclude name="**/?*.scala"/>
    <exclude name="**/?*.flex"/>
    <exclude name="**/?*.kt"/>
    <exclude name="**/?*.clj"/>
    <exclude name="**/?*.aj"/>
  </patternset>
  
  <!-- JDK definitions -->
  
  <property name="jdk.bin.11" value="${jdk.home.11}/bin"/>
  <path id="jdk.classpath.11">
    <fileset dir="${jdk.home.11}">
      <include name="../jdk-11.0.2!/java.base"/>
      <include name="../jdk-11.0.2!/java.compiler"/>
      <include name="../jdk-11.0.2!/java.datatransfer"/>
      <include name="../jdk-11.0.2!/java.desktop"/>
      <include name="../jdk-11.0.2!/java.instrument"/>
      <include name="../jdk-11.0.2!/java.logging"/>
      <include name="../jdk-11.0.2!/java.management"/>
      <include name="../jdk-11.0.2!/java.management.rmi"/>
      <include name="../jdk-11.0.2!/java.naming"/>
      <include name="../jdk-11.0.2!/java.net.http"/>
      <include name="../jdk-11.0.2!/java.prefs"/>
      <include name="../jdk-11.0.2!/java.rmi"/>
      <include name="../jdk-11.0.2!/java.scripting"/>
      <include name="../jdk-11.0.2!/java.se"/>
      <include name="../jdk-11.0.2!/java.security.jgss"/>
      <include name="../jdk-11.0.2!/java.security.sasl"/>
      <include name="../jdk-11.0.2!/java.smartcardio"/>
      <include name="../jdk-11.0.2!/java.sql"/>
      <include name="../jdk-11.0.2!/java.sql.rowset"/>
      <include name="../jdk-11.0.2!/java.transaction.xa"/>
      <include name="../jdk-11.0.2!/java.xml"/>
      <include name="../jdk-11.0.2!/java.xml.crypto"/>
      <include name="../jdk-11.0.2!/jdk.accessibility"/>
      <include name="../jdk-11.0.2!/jdk.aot"/>
      <include name="../jdk-11.0.2!/jdk.attach"/>
      <include name="../jdk-11.0.2!/jdk.charsets"/>
      <include name="../jdk-11.0.2!/jdk.compiler"/>
      <include name="../jdk-11.0.2!/jdk.crypto.cryptoki"/>
      <include name="../jdk-11.0.2!/jdk.crypto.ec"/>
      <include name="../jdk-11.0.2!/jdk.crypto.mscapi"/>
      <include name="../jdk-11.0.2!/jdk.dynalink"/>
      <include name="../jdk-11.0.2!/jdk.editpad"/>
      <include name="../jdk-11.0.2!/jdk.hotspot.agent"/>
      <include name="../jdk-11.0.2!/jdk.httpserver"/>
      <include name="../jdk-11.0.2!/jdk.internal.ed"/>
      <include name="../jdk-11.0.2!/jdk.internal.jvmstat"/>
      <include name="../jdk-11.0.2!/jdk.internal.le"/>
      <include name="../jdk-11.0.2!/jdk.internal.opt"/>
      <include name="../jdk-11.0.2!/jdk.internal.vm.ci"/>
      <include name="../jdk-11.0.2!/jdk.internal.vm.compiler"/>
      <include name="../jdk-11.0.2!/jdk.internal.vm.compiler.management"/>
      <include name="../jdk-11.0.2!/jdk.jartool"/>
      <include name="../jdk-11.0.2!/jdk.javadoc"/>
      <include name="../jdk-11.0.2!/jdk.jcmd"/>
      <include name="../jdk-11.0.2!/jdk.jconsole"/>
      <include name="../jdk-11.0.2!/jdk.jdeps"/>
      <include name="../jdk-11.0.2!/jdk.jdi"/>
      <include name="../jdk-11.0.2!/jdk.jdwp.agent"/>
      <include name="../jdk-11.0.2!/jdk.jfr"/>
      <include name="../jdk-11.0.2!/jdk.jlink"/>
      <include name="../jdk-11.0.2!/jdk.jshell"/>
      <include name="../jdk-11.0.2!/jdk.jsobject"/>
      <include name="../jdk-11.0.2!/jdk.jstatd"/>
      <include name="../jdk-11.0.2!/jdk.localedata"/>
      <include name="../jdk-11.0.2!/jdk.management"/>
      <include name="../jdk-11.0.2!/jdk.management.agent"/>
      <include name="../jdk-11.0.2!/jdk.management.jfr"/>
      <include name="../jdk-11.0.2!/jdk.naming.dns"/>
      <include name="../jdk-11.0.2!/jdk.naming.rmi"/>
      <include name="../jdk-11.0.2!/jdk.net"/>
      <include name="../jdk-11.0.2!/jdk.pack"/>
      <include name="../jdk-11.0.2!/jdk.rmic"/>
      <include name="../jdk-11.0.2!/jdk.scripting.nashorn"/>
      <include name="../jdk-11.0.2!/jdk.scripting.nashorn.shell"/>
      <include name="../jdk-11.0.2!/jdk.sctp"/>
      <include name="../jdk-11.0.2!/jdk.security.auth"/>
      <include name="../jdk-11.0.2!/jdk.security.jgss"/>
      <include name="../jdk-11.0.2!/jdk.unsupported"/>
      <include name="../jdk-11.0.2!/jdk.unsupported.desktop"/>
      <include name="../jdk-11.0.2!/jdk.xml.dom"/>
      <include name="../jdk-11.0.2!/jdk.zipfs"/>
    </fileset>
  </path>
  
  <property name="project.jdk.home" value="${jdk.home.11}"/>
  <property name="project.jdk.bin" value="${jdk.bin.11}"/>
  <property name="project.jdk.classpath" value="jdk.classpath.11"/>
  
  
  <!-- Project Libraries -->
  
  <path id="library.simplegraphics.classpath">
    <pathelement location="${basedir}/titanic2 4/lib/simplegraphics.jar"/>
  </path>
  
  <path id="library.unsikable.classpath">
    <pathelement location="${basedir}/titanic2 4/build/Unsikable.jar"/>
  </path>
  <!-- Register Custom Compiler Taskdefs -->
  <property name="javac2.home" value="${idea.home}/lib"/>
  <path id="javac2.classpath">
    <fileset dir="${javac2.home}">
      <include name="javac2.jar"/>
      <include name="jdom.jar"/>
      <include name="asm-all*.jar"/>
      <include name="forms-*.jar"/>
    </fileset>
  </path>
  <target name="register.custom.compilers">
    <taskdef name="javac2" classname="com.intellij.ant.Javac2" classpathref="javac2.classpath"/>
    <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  </target>
  
  <!-- Modules -->
  
  
  <!-- Module titanic2 4 -->
  
  <dirname property="module.titanic2_4.basedir" file="${ant.file}"/>
  
  
  <property name="module.jdk.home.titanic2_4" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.titanic2_4" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.titanic2_4" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.titanic2_4" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="titanic2_4.output.dir" value="${module.titanic2_4.basedir}/out/production/titanic2 4"/>
  <property name="titanic2_4.testoutput.dir" value="${module.titanic2_4.basedir}/out/test/titanic2 4"/>
  
  <path id="titanic2_4.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="titanic2_4.module.production.classpath">
    <path refid="${module.jdk.classpath.titanic2_4}"/>
    <path refid="library.simplegraphics.classpath"/>
    <path refid="library.unsikable.classpath"/>
  </path>
  
  <path id="titanic2_4.runtime.production.module.classpath">
    <pathelement location="${titanic2_4.output.dir}"/>
    <path refid="library.simplegraphics.classpath"/>
    <path refid="library.unsikable.classpath"/>
  </path>
  
  <path id="titanic2_4.module.classpath">
    <path refid="${module.jdk.classpath.titanic2_4}"/>
    <pathelement location="${titanic2_4.output.dir}"/>
    <path refid="library.simplegraphics.classpath"/>
    <path refid="library.unsikable.classpath"/>
  </path>
  
  <path id="titanic2_4.runtime.module.classpath">
    <pathelement location="${titanic2_4.testoutput.dir}"/>
    <pathelement location="${titanic2_4.output.dir}"/>
    <path refid="library.simplegraphics.classpath"/>
    <path refid="library.unsikable.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.titanic2_4">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.titanic2_4">
    <patternset refid="excluded.from.module.titanic2_4"/>
  </patternset>
  
  <path id="titanic2_4.module.sourcepath">
    <dirset dir="${module.titanic2_4.basedir}/titanic2 4">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.titanic2_4" depends="compile.module.titanic2_4.production,compile.module.titanic2_4.tests" description="Compile module titanic2 4"/>
  
  <target name="compile.module.titanic2_4.production" depends="register.custom.compilers" description="Compile module titanic2 4; production classes">
    <mkdir dir="${titanic2_4.output.dir}"/>
    <javac2 destdir="${titanic2_4.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.titanic2_4}/javac">
      <compilerarg line="${compiler.args.titanic2_4}"/>
      <bootclasspath refid="titanic2_4.module.bootclasspath"/>
      <classpath refid="titanic2_4.module.production.classpath"/>
      <src refid="titanic2_4.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.titanic2_4"/>
    </javac2>
    
    <copy todir="${titanic2_4.output.dir}">
      <fileset dir="${module.titanic2_4.basedir}/titanic2 4/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.titanic2_4.tests" depends="register.custom.compilers,compile.module.titanic2_4.production" description="compile module titanic2 4; test classes" unless="skip.tests"/>
  
  <target name="clean.module.titanic2_4" description="cleanup module">
    <delete dir="${titanic2_4.output.dir}"/>
    <delete dir="${titanic2_4.testoutput.dir}"/>
  </target>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.titanic2_4" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.titanic2_4" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
</project>